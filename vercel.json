{
    "version": 2,
    "builds": [
      {
        "src": "neplink/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "/media/$1"
      },
      {
        "src": "/admin/(.*)",
        "dest": "/neplink/wsgi.py/admin/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/neplink/wsgi.py/$1"
      }
    ]
  }
  